# Real Estate Dashboard Configuration
# React 앱 환경 변수 (REACT_APP_ 접두사 필요)

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_TIMEOUT=10000

# Map Integration
REACT_APP_KAKAO_MAP_API_KEY=your-kakao-map-api-key-here
REACT_APP_NAVER_MAP_API_KEY=your-naver-map-api-key-here

# Feature Flags
REACT_APP_ENABLE_ADMIN_MODE=false
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_ANALYTICS=true

# Performance Configuration
REACT_APP_QUERY_STALE_TIME=300000
REACT_APP_QUERY_CACHE_TIME=900000
REACT_APP_SYSTEM_HEALTH_REFETCH_INTERVAL=30000
REACT_APP_STATS_REFETCH_INTERVAL=60000

# UI Configuration
REACT_APP_ITEMS_PER_PAGE=20
REACT_APP_MAX_SEARCH_RESULTS=100
REACT_APP_DEBOUNCE_DELAY=500

# Theme Configuration
REACT_APP_THEME_MODE=light
REACT_APP_PRIMARY_COLOR=#1a1a2e
REACT_APP_SECONDARY_COLOR=#64b5f6

# Error Handling
REACT_APP_ERROR_REPORTING_ENABLED=false
REACT_APP_SENTRY_DSN=your-sentry-dsn-here

# Development Configuration
GENERATE_SOURCEMAP=true
BROWSER=none
FAST_REFRESH=true